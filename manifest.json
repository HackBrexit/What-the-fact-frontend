{
  "manifest_version": 2,

  "name": "What the fact selector",
  "description": "",
  "version": "0.9",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.1.1.min.js", "content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "activeTab",
    "tts",
    "http://*/",
    "https://*/"
  ]
}
